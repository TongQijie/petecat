{
  "routing": [
    {
      "key": "default",
      "value":  "index.html"
    }
  ],
  "resource": [
    {
      "key": "js",
      "value": "application/x-javascript"
    },
    {
      "key": "css",
      "value": "text/css"
    },
    {
      "key": "html",
      "value": "text/html"
    }
  ],
  "redirects": [
    {
      "pattern": "http://localhost:8080",
      "redirect": "https://localhost:8080",
      "mode": "replace"
    }
  ]
}